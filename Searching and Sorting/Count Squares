
//User function Template for Java

class Solution {
    static int countSquares(int N) {
        /*BRUTE FORCE
        int count = 0;
       for(int i=1;i<=N;i++){
           if(i*i < N) count++;
       }
       return count;
       */
       
    int i = 1, result = 1; 
    while (result < N) 
    { 
      i++; 
      result = i * i; 
    } 
    return i - 1; 
    }
}
