package encodingDecember2020;
import java.util.*;
public class MorningAssembly {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int T = scan.nextInt();
		for(int t=0;t<T;t++) {
			long r = scan.nextLong();
			long c = scan.nextLong();
			
			System.out.println(r*c);
		}

	}

}
